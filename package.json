{
    "name": "react-web-template",
    "private": true,
    "version": "0.0.0",
    "type": "module",
    "scripts": {
        "dev": "vite",
        "build": "tsc && vite build",
        "build:watch": "tsc && vite build --watch",
        "test": "jest --config=./jest.config.js --coverage=true --passWithNoTests",
        "test:watch": "jest --config=./jest.config.js --coverage=true --passWithNoTests --watchAll",
        "clean:dist": "npx rimraf dist",
        "lint:js": "eslint './src/**/*.{js,jsx,ts,tsx}'",
        "lint:css": "stylelint './src/**/*.{css}' -c=./stylelint.config.js --allow-empty-input",
        "lint": "yarn lint:js && yarn lint:css"
    },
    "dependencies": {
        "core-js": "^3.36.0",
        "react": "^18.2.0",
        "react-dom": "^18.2.0"
    },
    "devDependencies": {
        "@babel/core": "^7.23.2",
        "@babel/eslint-parser": "^7.22.15",
        "@babel/plugin-proposal-class-properties": "^7.18.6",
        "@babel/plugin-proposal-decorators": "^7.23.2",
        "@babel/preset-react": "^7.22.15",
        "@types/core-js": "^2.5.7",
        "@types/jest": "^29.5.7",
        "@types/node": "^20.8.10",
        "@types/react": "^18.2.15",
        "@types/react-dom": "^18.2.7",
        "@typescript-eslint/eslint-plugin": "^6.9.1",
        "@typescript-eslint/parser": "^6.9.1",
        "@vitejs/plugin-react": "^4.0.3",
        "autoprefixer": "^10.4.16",
        "eslint": "^8.53.0",
        "eslint-config-prettier": "^9.0.0",
        "eslint-plugin-prettier": "^5.0.1",
        "eslint-plugin-react": "^7.33.2",
        "eslint-plugin-react-hooks": "^4.6.0",
        "eslint-plugin-react-refresh": "^0.4.3",
        "eslint-scope": "^7.2.2",
        "jest": "^29.7.0",
        "postcss": "^8.4.31",
        "prettier": "^3.0.3",
        "stylelint": "^16.2.1",
        "tailwindcss": "^3.3.5",
        "ts-jest": "^29.1.1",
        "typescript": "^5.0.2",
        "vite": "^4.5.2",
        "vite-plugin-babel": "^1.1.3"
    },
    "resolutions": {
        "strip-ansi": "^6.0.1",
        "string-width": "^4.2.2",
        "wrap-ansi": "^7.0.0"
    }
}