{
    "root": true,
    "env": {
        "node": true
    },
    "extends": ["plugin:react/recommended", "eslint:recommended", "plugin:prettier/recommended"],
    "parserOptions": {
        "parser": "@typescript-eslint/parser",
        "ecmaVersion": 2020,
        "babelOptions": {
            "presets": ["@babel/preset-react"]
        }
    },
    "rules": {
        "no-console": "warn",
        "no-debugger": "warn"
    }
}
